{
	// Required
	"manifest_version": 2,
	"name": "Ohio State Rate My Professor Extension - Course Search Page",
	"version": "1.0.0.0",

	// Recommended
	//"default_locale": "en",
	"description": "An extensin that populates Rate My Professor information for CSE faculty at Ohio State University",
	"browser_action": {
		"default_icon": "icon.png"
	},
	"icons": {
		"16": "icon.png"
		},

	"author": "Greg Garcia",
	"background": {
		"scripts": ["eventPage.js"],
		//Recommended
		"persistent": false
	},

	"content_scripts": [
	{
		// fix this
		"matches": ["https://classes.osu.edu/class-search/*"],
		"js": ["jquery-3.0.0.min.js", "osurmp.js"]
	}
	],

	"permissions": [
		"http://www.ratemyprofessors.com/*",
		"tabs",
		"webNavigation"
	],

	"web_accessible_resources": [
		"http://www.ratemyprofessors.com/*"
	]

}